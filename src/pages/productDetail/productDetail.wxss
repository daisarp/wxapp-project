page {
  height: 100%;
  background: #F4F4F4;
}

.container {
  padding: 0.533333rem;
}

.borderBo {
  position: relative;
}

.borderBo:before {
  content: '';
  width: 200%;
  height: 0;
  position: absolute;
  left: 0;
  bottom: 0;
  border-bottom: 1px solid #D6D6D6;
  transform: scale(0.5);
  transform-origin: 0 0;
}

.borderTop {
  position: relative;
}

.borderTop:before {
  content: '';
  width: 200%;
  height: 0;
  position: absolute;
  left: 0;
  top: 0;
  border-bottom: 1px solid #D6D6D6;
  transform: scale(0.5);
  transform-origin: 0 0;
}

.pannel {
  margin-bottom: 0.533333rem;
  padding-top: 0.533333rem;
  position: relative;
  border-radius: 10px;
}

.pannel:before {
  content: '';
