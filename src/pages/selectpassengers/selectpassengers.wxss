page {
  height: 100%;
}

.head {
  width: 100%;
  height: 4.533333rem;
  background-color: #fff;
  border-bottom: 1px solid #cbcbcc;
  margin-bottom: 1.333333rem;
}

.head .addIcon {
  display: block;
  width: 1.546667rem;
  height: 1.546667rem;
  padding: 0.666667rem 0;
  margin: 0 auto;
}

.head .cjr {
  font-size: 28rpx;
  color: #929fb1;
  text-align: center;
  height: 1.653333rem;
}

.wrap {
  width: 100%;
  border-top: 1px solid #cbcbcc;
  border-bottom: 1px solid #cbcbcc;
  background: #FFFFFF;
}

.wrap .sum {
  margin: 0 auto;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-direction: row;
      flex-direction: row;
  -ms-flex-align: center;
      align-items: center;
}

.wrap .sum .sum-left {
  width: 105rpx;
  height: 98rpx;
}

.wrap .sum .sum-left .ch-img {
  width: 46rpx;
  height: 46rpx;
  margin-top: 26rpx;
  margin-left: 35rpx;
}

.wrap .sum:last-child .user {
  border-bottom: none;
}

.wrap .sum .user {
  display: -ms-flexbox;
  display: flex;
  -ms-flex-direction: row;
      flex-direction: row;
  -ms-flex-align: center;
      align-items: center;
  -ms-flex: 1;
      flex: 1;
  border-bottom: 1px solid #cbcbcc;
  width: 100%;
  font-size: 28rpx;
  overflow: hidden;
}

.wrap .sum .user .user-left {
  width: 46rpx;
  height: 46rpx;
  margin-top: 26rpx;
}

.wrap .sum .user .user-right {
  -ms-flex: 1;
      flex: 1;
  margin: 13rpx 0 0 10rpx;
}

.wrap .sum .user .redack {
  width: 140rpx;
  height: 98rpx;
}

.wrap .sum .user .redack .red-img {
  width: 58rpx;
  height: 60rpx;
  margin-top: 20rpx;
  margin-left: 35rpx;
}

.add {
  background: #59A5F0;
  color: #FFFFFF;
  width: 17.546667rem;
  font-size: 36rpx;
  height: 2.213333rem;
  text-align: center;
  line-height: 2.213333rem;
  margin-top: 1.706667rem;
  margin-bottom: 0.933333rem;
}